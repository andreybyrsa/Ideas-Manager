{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Assets/*": ["src/assets/*"],
      "@Components/*": ["src/components/*"],
      "@Layouts/*": ["src/layouts/*"],
      "@Pages/*": ["src/pages/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Store/*": ["src/store/*"],
      "@Services/*": ["src/services/*"],
      "@Middlewares/*": ["src/middlewares/*"],
      "@Utils/*": ["src/utils/*"]
    },
    "target": "ES6",
    "module": "CommonJS",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true
  }
}
